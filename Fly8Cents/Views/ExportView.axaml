<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="clr-namespace:Fly8Cents.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Fly8Cents.Views.ExportView"
             x:DataType="vm:ExportViewModel">
    <Grid Margin="10" RowDefinitions="*,Auto">
        <TextBox Grid.Row="0" AcceptsReturn="True" IsReadOnly="True" Text="这里显示生成的结果..." />
        <StackPanel Grid.Row="1" Orientation="Horizontal" HorizontalAlignment="Right" Margin="0,10,0,0">
            <Button Content="生成并保存" Margin="5,0" />
            <Button Content="复制到剪贴板" Margin="5,0" />
        </StackPanel>
    </Grid>
</UserControl>